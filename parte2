package main;

public class pruebacoche {
    public static void main(String[]args){
        Car myCar = new Car();
        for (int i=0; i<5; i++) myCar.AddDoor();
        System.out.print(myCar.doors);
     }
  
     public static int suma(int a, int b){
        return a + b;
     }
  }
  
  class Car {
  
     public int doors = 0;
     
     public void AddDoor(){
        this.doors++;
     }
     
  }
